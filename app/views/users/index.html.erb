<div class="usershow_box">

<div class="usershow">
<h2 class="mainarea-title">Partners</h2>


<% @users.each do |user| %>
<div>
<% if user.image.attached? %>
<%= image_tag user.image.variant(resize: '150x150') %>
<% else %>
<%= image_tag "partner.png", size: "150x150" %>
<% end %>
<%= user.username %>
<%= user.nationality %>
<%= user.profile %>
</div>
<% end %>

</div>
</div>